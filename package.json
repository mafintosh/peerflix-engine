{
  "name": "torrent-stream",
  "version": "1.0.3",
  "description": "Low level streaming torrent client that exposes files as node.js streams and downloads pieces based on demand",
  "repository": "git://github.com/mafintosh/torrent-stream.git",
  "scripts": {
    "build": "cross-env NODE_ENV=dev babel src --out-dir lib",
    "version": "cross-env NODE_ENV=dev npm run build",
    "test": "npm run build && tap test/*.js",
    "test-all": "npm run build && standard && tap test/*.js && npm run lint",
    "lint": "standard && eslint src test *.js"
  },
  "dependencies": {
    "babel-runtime": "^6.9.2",
    "bitfield": "^0.1.0",
    "bncode": "^0.5.2",
    "end-of-stream": "^0.1.4",
    "fs-chunk-store": "^1.3.0",
    "hat": "0.0.3",
    "immediate-chunk-store": "^1.0.5",
    "ip-set": "^1.0.0",
    "mkdirp": "^0.3.5",
    "parse-torrent": "^4.0.0",
    "peer-wire-swarm": "^0.12.0",
    "rimraf": "^2.2.5",
    "torrent-discovery": "^5.2.0",
    "torrent-piece": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-umd": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "bittorrent-tracker": "^2.6.0",
    "chai": "^3.5.0",
    "cross-env": "^1.0.8",
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1",
    "fs-extra": "^0.26.4",
    "mocha": "^2.5.3",
    "standard": "^5.1.0",
    "tap": "^0.4.8",
    "webpack": "^1.13.1"
  }
}
